---
import PreviewCode from "@components/ui/PreviewCode.astro";
import type { DemoDefinition } from "./createDemo";

const {
  demo,
  component: Component
} = Astro.props as {
  demo: DemoDefinition;
  component: any;
};
---

<PreviewCode label={demo.label} sources={demo.sources}>
  <Component slot="preview" />
</PreviewCode>
